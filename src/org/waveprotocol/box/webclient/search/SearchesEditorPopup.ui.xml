
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:w="urn:import:org.waveprotocol.wave.client.widget.common">
  <ui:with field='css'
    type='org.waveprotocol.box.webclient.search.SearchesEditorPopup.Css' />
  <ui:with field='msg'
    type='org.waveprotocol.box.webclient.search.i18n.SearchesEditorMessages' />
  <g:HTMLPanel styleName="{css.self}">
    <div class="{css.centerPanel}">
      <div class="{css.controlButtonsPanel}">
        <g:VerticalPanel>
          <g:Button ui:field='addButton' styleName="{css.controlButton}">
            <ui:text from="{msg.add}"/>
          </g:Button>
          <g:Button ui:field='modifyButton' styleName="{css.controlButton}">
            <ui:text from="{msg.modify}"/>
          </g:Button>
          <g:Button ui:field='removeButton' styleName="{css.controlButton}">
            <ui:text from="{msg.remove}"/>
          </g:Button>
          <g:Button ui:field='upButton' styleName="{css.controlButton}">
            <ui:text from="{msg.up}"/>
          </g:Button>
          <g:Button ui:field='downButton' styleName="{css.controlButton}">
            <ui:text from="{msg.down}"/>
          </g:Button>
        </g:VerticalPanel>
      </div>
      <div class="{css.searchesPanel}">
        <g:ListBox ui:field='searchesListBox' styleName="{css.searchesListBox}"></g:ListBox>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>
